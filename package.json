{
  "name": "viztory",
  "description": "visual house price",
  "version": "1.0.0",
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && rm -rf /build && npm run build",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mysql": "^2.15.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "concurrently": "^3.5.1"
  }
}
